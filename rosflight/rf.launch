<?xml version="1.0"?>

<launch>
  <!-- FCU communication -->
  <node name="rosflight_io" pkg="rosflight" type="rosflight_io" output="screen">
    <param name="udp" value="false"/>
  </node>

  <!-- Load common roscopter parameters -->
  <!-- <include file="/home/ohrad/catkin_ws/src/roscopter/roscopter/launch/load_params.launch" /> -->

  <node pkg="rosservice" type="rosservice" name="cal_imu" args="call /calibrate_imu" output="screen"/>
  <node pkg="rosservice" type="rosservice" name="load_params" args="call /param_load_from_file /home/ohrad/subt_rf_params.yml" output="screen"/>

</launch>
